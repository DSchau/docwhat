  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  },
  "dependencies": {
    "babel-eslint": "^8.0.3",
    "broken-link-checker": "^0.7.8",
    "caniuse-db": "^1.0.30000815",
