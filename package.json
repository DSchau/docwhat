{
  "name": "docwhat.org",
  "description": "Personal blog of Christian Höltje",
  "version": "1.0.0",
  "author": "Christian Höltje <docwhat@gerf.org>",
  "bugs": {
    "url": "https://github.com/docwhat/docwhat/issues"
  },
  "jshintConfig": {
    "esversion": 6,
    "asi": true
  },
  "dependencies": {
    "babel-eslint": "^8.0.3",
    "broken-link-checker": "^0.7.6",
    "color": "^3.0.0",
    "compass-vertical-rhythm": "^1.4.0",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "gatsby": "1.9.215",
    "gatsby-cli": "^1.1.44",
    "gatsby-link": "1.6.37",
    "gatsby-plugin-feed": "1.3.18",
    "gatsby-plugin-google-analytics": "1.0.19",
    "gatsby-plugin-offline": "1.0.14",
    "gatsby-plugin-react-helmet": "1.0.8",
    "gatsby-plugin-sharp": "^1.6.21",
    "gatsby-plugin-sitemap": "^1.2.14",
    "gatsby-plugin-typography": "1.7.14",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-copy-linked-files": "1.5.28",
    "gatsby-remark-emoji": "^0.0.1",
    "gatsby-remark-images": "1.5.51",
    "gatsby-remark-prismjs": "1.2.15",
    "gatsby-remark-responsive-iframe": "1.4.17",
    "gatsby-remark-smartypants": "1.4.11",
    "gatsby-source-filesystem": "1.5.24",
    "gatsby-transformer-remark": "^1.7.33",
    "gatsby-transformer-sharp": "^1.6.14",
    "gatsby-transformer-yaml": "^1.5.16",
    "gh-pages": "^1.1.0",
    "lodash": "4.17.5",
    "prettier": "^1.8.2",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-gist": "^1.2.1",
    "react-gravatar": "^2.6.3",
    "react-icons": "2.2.7",
    "rehype-react": "^3.0.2",
    "typography-breakpoint-constants": "^0.15.10",
    "typography-theme-fairy-gates": "^0.15.11",
    "typography-theme-github": "^0.15.10"
  },
  "homepage": "https://github.com/docwhat/docwhat#readme",
  "keywords": [
    "gatsby",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/docwhat/docwhat.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "check-internal-links": "broken-link-checker http://localhost:8000/ --recursive --ordered --exclude-external --filter-level=3",
    "check-external-links": "broken-link-checker http://localhost:8000/ --recursive --ordered --exclude-internal --filter-level=3",
    "check-links": "broken-link-checker --ordered --filter-level=3",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  }
}
